{
  "manifest_version": 2,
  "name": "Github <3 Courier",
  "description": "Revert github code font back to Courier",
  "version": "0.0.1",
  "content_scripts": [{
    "run_at": "document_end",
    "matches": [
      "http://*.github.com/**/*", 
      "https://*.github.com/**/*"
    ],
    "css": [
      "index.css"
    ]
  }]
}
